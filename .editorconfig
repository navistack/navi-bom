root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 120

[*.{java,xml}]
charset = utf-8
indent_style = space
indent_size = 4
continuation_indent_size = 8

[*.md]
charset = utf-8
insert_final_newline = false
trim_trailing_whitespace = false
